/* pages/photo/photo.wxss */
@import '../index/index.wxss';
.photo {
    width: 100%;
    height: 100%;
    margin-top: 120rpx;
    box-sizing: border-box;
    height: calc(100% - 120rpx);
    padding-bottom: 10rpx
}

swiper{
    width: 100%;
    height: 100%;
}

movable-view.show{
    animation: fadeIn 0.5s ease-in;
    animation-fill-mode: forwards;
}
movable-view.hide{
    animation: fadeOut 0.5s ease-in;
    animation-fill-mode: forwards;
}

@keyframes fadeIn{
    0%{
        opacity: 0;
    }
    100%{
        opacity: 1;
    }
}
@keyframes fadeOut{
    0%{
        opacity: 1;
    }
    100%{
        opacity: 0;
    }
}